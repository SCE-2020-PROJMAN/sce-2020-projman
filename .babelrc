{
    "presets": [
        ["@babel/preset-env", {
            "modules": "commonjs",
            "useBuiltIns": "usage",
            "corejs": {"version": 3, "proposals": true}
        }]
    ],
    "plugins": [
        ["babel-plugin-transform-builtin-extend", {"globals": ["Error"]}],
        "@babel/transform-runtime"
    ],
    "ignore": [
        "src/api/node_modules"
    ]
}
